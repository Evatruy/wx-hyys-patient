  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-bottom: 130rpx;
}
.view-li{
  margin-top: 40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.view-li image{
  width: 330rpx;
  height:220rpx;
}
.view-li view{
  padding-top: 24rpx;
  color: #374AA0;
  max-width: 300rpx;
  text-align: center;
}
.none-msg{
  text-align: center;
  padding:20rpx 0;
  color: #999;
}