/* pages/component/navigate/index.wxss */
.nav-wrap{
  width: 100%;
  background: linear-gradient(90deg, #00D190 0%, #00E3A4 100%);
  display: flex;
  flex-direction: column;
}

.nav-back{
  position: absolute;
  bottom: 0%;
  padding: 20rpx 30rpx;
}

.nav-search{
  line-clamp: 50px;
  direction: block;
  margin-top: 30px;
  margin-left: 90rpx;
  width: 60%;
}

.search-group {
  width: 80%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border: 4rpx solid #ffffff;
  border-radius: 60rpx;
  padding: 0rpx 15rpx;
}

.search-group > input {
  font-size: 25rpx;
  background-color: transparent;
  color: #ffffff;
}

.phcolor {
  color: #ffffff;
}